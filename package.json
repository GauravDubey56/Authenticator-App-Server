    "pg": "^8.11.3",
    "query-string": "^8.1.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.0",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
